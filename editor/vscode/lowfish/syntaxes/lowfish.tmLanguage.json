{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "lowfish",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#support"
		},
		{
			"include": "#variables"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.lf",
					"match": "\\b(enum|struct|if|then|else|true|false|void|u16|i16|u32|i32|u8|i8|u64|i64)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.lf",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.lowfish",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"name": "constant.numeric.lf",
			"match": "\\\\([0-9]+) '.' ([0-9]+)?\\\\"
		}
	},
	"scopeName": "source.lf"
}